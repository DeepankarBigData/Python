number = 5
factorial = 1

if number < 0:
    print("Factorial is not defined for negative numbers")
elif number == 0:
    print("The factorial of 0 is 1")
else:
    for i in range(1, number + 1):
        factorial *= i
    print(f"The factorial of {number} is {factorial}")



g = 5
p = 5

while g>=2:
    g = (g-1)
    # print(g)
    p = p*g
    
print(p)
	
